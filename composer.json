{
    "name": "robofirm/devops-tool-core",
    "description": "Robofirm DevOps Tool: Core",
    "type": "library",
    "homepage": "https://bitbucket.org/robofirm/devops-tool-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirk Madera",
            "email": "kmadera@robofirm.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-pdo_mysql": "*",
        "monolog/monolog": "^1.21.0",
        "league/flysystem": "^1.0",
        "emgag/flysystem-hash": "^1.0.1",
        "league/flysystem-sftp": "^1.0",
        "aws/aws-sdk-php": "^3.18.39",
        "symfony/monolog-bridge": "^3.2"
    },
    "suggest": {
        "robofirm/devops-tool-application-orchestration": "Adds application orchestration functionality.",
        "robofirm/devops-tool-project-create": "Adds project creation functionality from Jira/Confluence project setup to Terraform/Puppet/DevOps Tool repo setup.",
        "robofirm/devops-tool-mysql-database-support": "Adds support for working with MySQL databases.",
        "robofirm/devops-tool-aws-s3-filesystem-support": "Adds support for working with AWS S3 filesystems."
    },
    "autoload": {
        "psr-4": {
            "DevopsToolCore\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "config-provider": "DevopsToolCore\\ConfigProvider"
        }
    },
    "bin": [
        "bin/devops"
    ]
}
